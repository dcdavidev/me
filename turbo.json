{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".react-router/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".react-router/**", "dist/**"]
    }
  }
}
